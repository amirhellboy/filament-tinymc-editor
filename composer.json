{
    "name": "amirhellboy/filament-tinymce-editor",
    "description": "A TinyMce editor component for Filament",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "tinyeditor",
        "tinymce"
    ],
    "authors": [
        {
            "name": "amirhellboy",
            "email": "amirhellboy31@yahoo.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Amirhellboy\\FilamentTinymceEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amirhellboy\\FilamentTinymceEditor\\TinymceditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
